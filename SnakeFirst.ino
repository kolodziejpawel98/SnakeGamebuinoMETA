Image fruitImg(fruitImgCode);
Image snakeBodyCellImg(snakeBodyCellImgCode);
Image heartImg[MAX_HEALTH] = {heartImgCode, heartImgCode, heartImgCode };
Image redScreenImg(redScreenImgCode);
Image homeScreenImg(homeScreenImgCode);

void resetSettings() {
    currentHealth = 3;
    score = 0;
    snakeBody.snakeBodyArr.clear();
    snakeBody.addBodySegment(HEAD_X, HEAD_Y);
    fruitX = -1;
    fruitY = -1;
}

void setup() {
    gb.begin();
    snakeBody.addBodySegment(HEAD_X, HEAD_Y);
    snakeBody.headToFirst();
    //gb.setFrameRate(15);
    stateOfGame = HOME_SCREEN_STATE;
}

void loop() {
    while (!gb.update());
    gb.display.clear();



    switch (stateOfGame) {
        case HOME_SCREEN_STATE:
            gb.display.println("Press 'A' to play");
            gb.display.drawImage(0, 6, homeScreenImg);
            if(gb.buttons.pressed(BUTTON_A)){
                stateOfGame = PLAY_STATE;
            }
            break;
        case PLAY_STATE:
            setColor(color);
            for (short i = 0, j = 0; i < currentHealth; i++, j += 6) {
                gb.display.drawImage(50 + j, 0, heartImg[i]);
            }
            moveButtonListener();
            putFruit();
            snakeMove(snakeDirection);
            gameOverCheck();
            gb.display.printf("score: %d", score);
            //gb.display.printf("%d %d | %d %d", fruitX, fruitY, snakeBody.snakeBodyArr[0].x, snakeBody.snakeBodyArr[0].y);
            break;
        case GAME_OVER_STATE:
            gb.display.println("GAME OVER!");
            gb.display.printf("You're score: %d", score);
            gb.display.println("Press 'A' to play again");
            gb.display.println("Press 'B' to exit");
            if (gb.buttons.pressed(BUTTON_A)) {
                resetSettings();
                stateOfGame = PLAY_STATE;
            }else if (gb.buttons.pressed(BUTTON_B)) {
                resetSettings();
                stateOfGame = HOME_SCREEN_STATE;
            }
            break;
    }
}

void moveButtonListener() {
    if (gb.buttons.pressed(BUTTON_UP)) {
        if (snakeDirection == DOWN) {
            snakeBody.headTailFlip();
        }
        snakeDirection = UP;
    }
    if (gb.buttons.pressed(BUTTON_RIGHT)) {
        if (snakeDirection == LEFT) {
            snakeBody.headTailFlip();
        }
        snakeDirection = RIGHT;
    }
    if (gb.buttons.pressed(BUTTON_LEFT)) {
        if (snakeDirection == RIGHT) {
            snakeBody.headTailFlip();
        }
        snakeDirection = LEFT;
    }
    if (gb.buttons.pressed(BUTTON_DOWN)) {
        if (snakeDirection == UP) {
            snakeBody.headTailFlip();
        }
        snakeDirection = DOWN;
    }
}

void snakeMove(int snakeDirection) {

    if (snakeBody.snakeBodyArr[0].x >= SCREEN_RIGHT_BORDER)
        snakeBody.snakeBodyArr[0].x = SCREEN_LEFT_BORDER + CELL_SIZE;
    if (snakeBody.snakeBodyArr[0].x <= SCREEN_LEFT_BORDER)
        snakeBody.snakeBodyArr[0].x = SCREEN_RIGHT_BORDER - CELL_SIZE;
    if (snakeBody.snakeBodyArr[0].y >= SCREEN_BOTTOM_BORDER)
        snakeBody.snakeBodyArr[0].y = SCREEN_TOP_BORDER + CELL_SIZE;
    if (snakeBody.snakeBodyArr[0].y <= SCREEN_TOP_BORDER)
        snakeBody.snakeBodyArr[0].y = SCREEN_BOTTOM_BORDER - CELL_SIZE;


    switch (snakeDirection) {
    case UP:
        snakeBody.moveUp();
        break;
    case RIGHT:
        snakeBody.moveRight();
        break;
    case LEFT:
        snakeBody.moveLeft();
        break;
    case DOWN:
        snakeBody.moveDown();
        break;
    }
    for (short i = 0; i < snakeBody.getSize(); i++) {
        //gb.display.fillRect(snakeBody.snakeBodyArr[i].x, snakeBody.snakeBodyArr[i].y, CELL_SIZE, CELL_SIZE);
        gb.display.drawImage(snakeBody.snakeBodyArr[i].x, snakeBody.snakeBodyArr[i].y, snakeBodyCellImg);
    }
}

void putFruit() {
    if (fruitX == -1 && fruitY == -1) {
        fruitX = 4 * random(SCREEN_LEFT_BORDER, (SCREEN_RIGHT_BORDER - CELL_SIZE + 1)/4); // [0, 80-4+1]
        fruitY = 4 * random(SCREEN_TOP_BORDER, (SCREEN_BOTTOM_BORDER - CELL_SIZE + 1)/4); //[6, 64-4+1]
    }
    byte previousColor = color;
    if (gb.collide.rectRect(fruitX, fruitY, CELL_SIZE, CELL_SIZE, snakeBody.snakeBodyArr[0].x, snakeBody.snakeBodyArr[0].y, CELL_SIZE, CELL_SIZE)) {
        gb.sound.playTick();

        while (previousColor == color)
            color = random(0, 4);

        fruitX = 4 * random(SCREEN_LEFT_BORDER, (SCREEN_RIGHT_BORDER - CELL_SIZE + 1) / 4); // [0, 80-4+1]
        fruitY = 4 * random(SCREEN_TOP_BORDER, (SCREEN_BOTTOM_BORDER - CELL_SIZE + 1) / 4); //[6, 64-4+1]


        switch (snakeDirection) {
        case UP:
            snakeBody.addBodySegment(snakeBody.snakeBodyArr[snakeBody.getSize() - 1].x, snakeBody.snakeBodyArr[snakeBody.getSize() - 1].y + CELL_SIZE);
            break;
        case RIGHT:
            snakeBody.addBodySegment(snakeBody.snakeBodyArr[snakeBody.getSize() - 1].x - CELL_SIZE, snakeBody.snakeBodyArr[snakeBody.getSize() - 1].y);
            break;
        case LEFT:
            snakeBody.addBodySegment(snakeBody.snakeBodyArr[snakeBody.getSize() - 1].x + CELL_SIZE, snakeBody.snakeBodyArr[snakeBody.getSize() - 1].y);
            break;
        case DOWN:
            snakeBody.addBodySegment(snakeBody.snakeBodyArr[snakeBody.getSize() - 1].x, snakeBody.snakeBodyArr[snakeBody.getSize() - 1].y - CELL_SIZE);
            break;
        }

        score++;
    }
    //gb.display.fillRect(fruitX, fruitY, CELL_SIZE, CELL_SIZE);
    gb.display.drawImage(fruitX, fruitY, fruitImg);
}

void setColor(byte color) {
    switch (color) {
    case 0:
        gb.display.setColor(RED);
        break;
    case 1:
        gb.display.setColor(GREEN);
        break;
    case 2:
        gb.display.setColor(BLUE);
        break;
    case 3:
        gb.display.setColor(YELLOW);
        break;
    }
}

void gameOverCheck() {
    for (short i = 1; i < snakeBody.getSize(); i++) {
        if (snakeBody.snakeBodyArr[0].x == snakeBody.snakeBodyArr[i].x
            && snakeBody.snakeBodyArr[0].y == snakeBody.snakeBodyArr[i].y) {
            currentHealth--;
            gb.display.drawImage(0, 0, redScreenImg);
        }
    }
    if (currentHealth == 0) {
        stateOfGame = GAME_OVER_STATE;
    }
}
